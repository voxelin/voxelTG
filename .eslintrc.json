{
    "env": {
        "es2021": true,
        "node": true
    },
    "overrides": [],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/eslint-recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "plugins": ["prettier", "@typescript-eslint"]
}
